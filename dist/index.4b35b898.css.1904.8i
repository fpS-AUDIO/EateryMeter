.comingsoon--container {
  border-radius: var(--border-radius);
  box-shadow: var(--shadow-colors);
  background-color: var(--general-container-bg-color-2);
  transition: all var(--transition-time) ease;
  margin: 1rem;
  padding: 1rem;
  overflow: hidden;
}

@supports ((-webkit-backdrop-filter: blur(10px))) or (backdrop-filter: blur(10px)) {
  .comingsoon--container {
    backdrop-filter: blur(10px);
  }
}

.comingsoon--container:hover {
  box-shadow: var(--shadow-colors-hover);
}

.comingsoon--container .comingsoon--hr {
  background-color: var(--text-h1-color);
  border: 0;
  width: 75%;
  height: .1rem;
  margin: 1rem 0;
}
